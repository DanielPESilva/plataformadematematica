generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model modulo_has_turma {
  modulo_mod_id Int
  turma_tur_id  Int

  @@id([modulo_mod_id, turma_tur_id])
}

model questao {
  que_id         Int
  que_posicao    Int?
  que_titulo     String? @db.VarChar(45)
  que_pdf        String? @db.VarChar(255)
  que_link_video String? @db.VarChar(200)
  modulo_mod_id  Int

  @@id([que_id, modulo_mod_id])
}

model turma {
  tur_id     Int     @id
  tur_titulo String? @db.VarChar(45)
}

model turma_has_usuario {
  turma_tur_id   Int
  usuario_usu_id Int

  @@id([turma_tur_id, usuario_usu_id])
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model usuario {
  usu_id        Int     @id
  usu_nome      String  @db.VarChar(200)
  usu_tel_      String? @map("usu_tel.") @db.VarChar(45)
  alu_email     String  @db.VarChar(200)
  usu_matricula Int
  usu_cpf       String  @db.VarChar(45)
  usu_situacao  String  @db.VarChar(45)
}
